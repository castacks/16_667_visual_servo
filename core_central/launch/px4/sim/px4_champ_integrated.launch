<?xml version="1.0"?>
<launch>
  
  <!-- launch gazebo -->
  <include file="$(find core_central)/launch/common/gazebo_world.launch" >
    <arg name="set_px4_paths" value="true" />
    <!-- <arg name="output" value="log"/> -->
    <!-- <arg name="world" value="$(find core_gazebo_sim)/worlds/isaac_ocean_map.world" /> -->
    <!-- <arg name="world" value="$(find core_gazebo_sim)/worlds/isaac_ocean_map_edit.world" /> -->
    <arg name="world" value="$(find core_gazebo_sim)/worlds/issac_ocean_integrated.world" />
  </include>

  <!-- <include file="$(find cmu_uav_autonomy)/launch/sim.launch"/> -->


  <!-- <include file="$(find apriltag_ros)/launch/common/gazebo_world.launch">
    <arg name="set_px4_paths" value="true" />
  </include> -->

  <!-- create a px4 drone -->
  <include file="$(find core_central)/launch/px4/sim/px4_sim_drone_visual_servo.launch" pass_all_args="true" output="log">
    <arg name="robot_name" value="" />
    <arg name="ID" value="0"/>
    
    <arg name="x" value="-1100.014047166340625"/>
    <arg name="y" value="-1210.0828982581410025"/>
    <arg name="z" value="1.6726249328603324"/> 
    
    <!-- <arg name="Y" value="3.1415" /> -->
    
    <arg name="drone_interface" value="PX4TiltedHexInterface" />
    <arg name="map_representation" value="PointCloudMapRepresentation" />
  </include>

  <!-- run task planner -->
  <include file="$(find task_planner)/launch/task_planner_champ.launch"/>

  <!-- run visual servo -->
  <include file="$(find feature_extractor)/launch/circle_extractor.launch"/>




</launch>

